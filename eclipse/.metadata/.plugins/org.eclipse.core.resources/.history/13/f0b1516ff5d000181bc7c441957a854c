	public static String generateRandomName(Random rand, AttributeRace race, int gender)
	{
		if(race == null) return "";
		switch(race.getType())
		{
			case AttributeRace.RACE_TYPE_LATIN:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mLatinNames[rand.nextInt(mLatinNames.length)].trim();
				}
				else
				{
					return fLatinNames[rand.nextInt(fLatinNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_NORD:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mNordNames[rand.nextInt(mNordNames.length)].trim();
				}
				else
				{
					return fNordNames[rand.nextInt(fNordNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_GERMAN:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mGermanNames[rand.nextInt(mGermanNames.length)].trim();
				}
				else
				{
					return fGermanNames[rand.nextInt(fGermanNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_SLAV:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mSlavNames[rand.nextInt(mSlavNames.length)].trim();
				}
				else
				{
					return fSlavNames[rand.nextInt(fSlavNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_ARAB:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mArabNames[rand.nextInt(mArabNames.length)].trim();
				}
				else
				{
					return fArabNames[rand.nextInt(fArabNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_GREEK:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mGreekNames[rand.nextInt(mGreekNames.length)].trim();
				}
				else
				{
					return fGreekNames[rand.nextInt(fGreekNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_FRANK:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mFrankNames[rand.nextInt(mFrankNames.length)].trim();
				}
				else
				{
					return fFrankNames[rand.nextInt(fFrankNames.length)].trim();
				}
			}
			case AttributeRace.RACE_TYPE_BRITON:
			{
				if(gender == EntityHumanVillager.GENDER_MALE)
				{
					return mBritonNames[rand.nextInt(mBritonNames.length)].trim();
				}
				else
				{
					return fBritonNames[rand.nextInt(fBritonNames.length)].trim();
				}
			}
			default:
			{
				return "";
			}
		}
	}


	public static String generateRandomName(Random rand, AttributeRace race) {
		// TODO Auto-generated method stub
		if(race == null) return "";
		switch(race.getType())
		{
			case AttributeRace.RACE_TYPE_LATIN:
			{
				return lLatinNames[rand.nextInt(lLatinNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_NORD:
			{
				return lNordNames[rand.nextInt(lNordNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_SLAV:
			{
				return lSlavNames[rand.nextInt(lSlavNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_GERMAN:
			{
				return lGermanNames[rand.nextInt(lGermanNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_ARAB:
			{
				return lArabNames[rand.nextInt(lArabNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_GREEK:
			{
				return lGreekNames[rand.nextInt(lGreekNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_FRANK:
			{
				return lFrankNames[rand.nextInt(lFrankNames.length)].trim();
			}
			case AttributeRace.RACE_TYPE_BRITON:
			{
				return lBritonNames[rand.nextInt(lBritonNames.length)].trim();
			}
			default:
			{
				return "";
			}
		}
	}
}
